<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

    <classPathEntry location="${drive.class.path}" />

    <context id="DB2Tables" targetRuntime="MyBatis3" defaultModelType="flat">

        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>

        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="${jdbc.driver}"
        	connectionURL="${jdbc.url}"
        	userId="${jdbc.username}"
        	password="${jdbc.password}">
    	  </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="true"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="${model.package}" targetProject="${model.target.project}">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>



        <sqlMapGenerator targetPackage="${xml.mapper.package}" targetProject="${mapping.target.project}">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator targetPackage="${dao.package}" targetProject="${dao.target.project}" type="XMLMAPPER">
            <property name="enableSubPackages" value="true"/>
            <property name="rootInterface" value="com.eflag.esms.dao.BaseMapper"/>
        </javaClientGenerator>
        
        <!--
        <table tableName="%">
            <generatedKey column="id" sqlStatement="Mysql" identity="true" />
        </table>	
        <table tableName="apply_list_tab" domainObjectName="ApplyListTab">
        </table>
        <table tableName="apply_detail_tab" domainObjectName="ApplyDetailTab">
        </table>
        -->
        <!--<table tableName="warehouse_model_structure" domainObjectName="WarehouseModelStructure">-->
        <!--</table>-->
        <table tableName="inventory_positiond" domainObjectName="InventoryPositiond"></table>
    </context>
</generatorConfiguration>